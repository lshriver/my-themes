    "chokidar": "^3.6.0",
    "clap": "^3.1.1",
    "css-tree": "^2.2.1",
    "fill-range": "^7.1.1",
    "glob-parent": "^5.1.2",
    "is-binary-path": "^2.1.0",
    "is-extglob": "^2.1.1",
